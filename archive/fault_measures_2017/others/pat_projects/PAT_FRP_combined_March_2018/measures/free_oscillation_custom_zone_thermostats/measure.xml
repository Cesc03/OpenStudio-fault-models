  <class_name>FreeOscillationCustomZoneThermostats</class_name>
  <display_name>Free Oscillation Custom Zone Thermostats</display_name>
  <description>This is for use when you want to regulate zones to specific temperatures before cutting the HVAC system off for free oscillation analysis. A constat schedule is used for the entire run period. Zones that do not have a thermostat won't be altered</description>
  <modeler_description>Argument will be made for each zone in the model and another argument for the deadband size between the heating and cooling setpoints that will center on the entered temperature.</modeler_description>
  <arguments>
    <argument>
      <name>deadband</name>
      <display_name>Deadband Size</display_name>
      <type>Double</type>
      <units>F</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.2</default_value>
    </argument>
    <argument>
      <name>preset_shift</name>
      <display_name>Preset Shift</display_name>
      <type>Double</type>
      <units>F</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>-4</default_value>
    </argument>
  </arguments>
